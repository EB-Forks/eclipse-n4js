/* XPECT_SETUP org.eclipse.n4js.bugreports.tests.N4JSBugreportTest END_SETUP  */


// 'this'-type in signatures of static members ...

// ... disallowed in generic classes:

class G<T> {

	// XPECT errors --> "The this type isn't allowed on this place. (Please refer to Spec for valid use cases.)" at "this"	
	static m1(): this {
		// XPECT noerrors -->
		this;
		return undefined;
	}

	// XPECT errors --> "The this type isn't allowed on this place. (Please refer to Spec for valid use cases.)" at "this"	
	static m2(p: {function(this)}) {}

	// XPECT errors --> "The this type isn't allowed on this place. (Please refer to Spec for valid use cases.)" at "this"	
	static get g(): this {
		// XPECT noerrors -->
		this;
		return undefined;
	}

	// XPECT errors --> "The this type isn't allowed on this place. (Please refer to Spec for valid use cases.)" at "this"	
	static set s(p: {function(this)}) {}
}


// ... allowed in non-generic classes:

class C {

	// XPECT noerrors -->
	static m1(): this {
		// XPECT noerrors -->
		this;
		return undefined;
	}

	// XPECT noerrors -->
	static m2(p: {function(this)}) {}

	// XPECT noerrors -->
	static get g(): this {
		// XPECT noerrors -->
		this;
		return undefined;
	}

	// XPECT noerrors -->
	static set s(p: {function(this)}) {}
}

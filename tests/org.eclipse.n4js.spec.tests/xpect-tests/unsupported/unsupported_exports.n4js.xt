/* XPECT_SETUP org.eclipse.n4js.spec.tests.N4JSSpecTest

	IssueConfiguration {
		IssueCode "IMP_DEFAULT_EXPORT_DUPLICATE" { enabled = false }
	}

	END_SETUP
 */


class C {}
class D {}


// XPECT errors --> "Unsupported feature: anonymous default export." at "class {}"
export default class {}
// XPECT errors --> "Unsupported feature: anonymous default export." at "interface {}"
export default interface {}
// XPECT errors --> "Unsupported feature: anonymous default export." at "enum { YES, NO }"
export default enum { YES, NO }
// XPECT errors --> "Unsupported feature: anonymous default export." at "function() {}"
export default function() {}


// XPECT errors --> "Unsupported feature: separate export statements (add keyword 'export' directly before a class, interface, enum, function or variable declaration, or before an expression)." at "export {C} "
export {C}


// XPECT errors --> "Unsupported feature: separate export statements (add keyword 'export' directly before a class, interface, enum, function or variable declaration, or before an expression)." at "export {D as X} "
export {D as X}


// XPECT noerrors -->
export default "hello"


// no errors, except the one about unsupported class expressions:
// XPECT errors --> "Unsupported feature: class expressions." at "class"
export default (class {})

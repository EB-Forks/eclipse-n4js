{
  "name": "n4js-node",
  "version": "0.1.0",
  "author": {
    "name": "NumberFour AG",
    "email": "npm@numberfour.eu"
  },
  "license": "EPL-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse/n4js"
  },
  "description": "N4JS node.js runtime environment.",
  "keywords": [
    "n4js",
    "es6",
    "es2015",
    "transpiler",
    "ide",
    "n4jside",
    "n4js-ide"
  ],
  "engines": {
    "node": ">= 8.0.0"
  },
  "main": "./src-gen/index.js",
  "dependencies": {
    "n4js-es5": "^0.1.0",
    "n4js-runtime-es2015": "^0.1.0",
    "n4js-runtime-fetch": "^0.1.0"
  },
  "files": [
    "src",
    "src-gen"
  ],
  "n4js": {
    "useES6Imports": true,
    "projectType": "library",
    "vendorId": "org.eclipse.n4js",
    "vendorName": "Eclipse N4JS Project",
    "output": "src-gen",
    "sources": {
      "source": [
        "src/n4js"
      ],
      "external": [
        "src/js"
      ]
    },
    "requiredRuntimeLibraries": [
      "n4js-runtime-fetch",
      "n4js-runtime-es2015"
    ]
  }
}
